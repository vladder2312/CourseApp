<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="FeedToolbar">
        <item name="android:textCursorDrawable">@color/colorBlue</item>
        <item name="title"></item>
        <item name="android:textColor">@android:color/white</item>
        <item name="queryBackground">@android:color/transparent</item>
        <item name="searchIcon">@drawable/icon_search</item>
        <item name="closeIcon">@drawable/icon_close</item>
        <item name="queryHint">@string/search_hint</item>
        <item name="android:imeActionId">6</item>
    </style>
    <style name="ProfileToolbarTheme" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="menu">@menu/profile_menu</item>
        <item name="android:textColorPrimary">@color/colorAccent</item>
        <item name="android:textColorSecondary">@android:color/white</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:colorBackground">@color/colorPrimary</item>
    </style>
</resources>